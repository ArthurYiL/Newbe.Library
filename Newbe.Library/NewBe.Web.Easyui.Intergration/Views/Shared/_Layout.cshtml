<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - NewBe.Web.Easyui.Intergration</title>

    <environment names="Development">
        <link href="~/lib/easyui/themes/default/easyui.css" rel="stylesheet" />
        <link href="~/lib/easyui/themes/icon.css" rel="stylesheet" />
    </environment>
</head>
<body>
@RenderBody()
<environment names="Development">
    <script src="~/lib/underscore/underscore-min.js"></script>
    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="~/lib/easyui/jquery.easyui.min.js"></script>
    <script src="~/js/easyui.defaults.js"></script>
</environment>
@RenderSection("Scripts", required: false)
</body>
</html>
